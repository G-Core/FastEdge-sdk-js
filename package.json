{
  "name": "@FastEdge/sdk-js",
  "version": "0.0.1-beta",
  "engines": {
    "node": "18 - 20",
    "npm": "^8 || ^9"
  },
  "main": "componentize.js",
  "types": "types/index.d.ts",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/godronus/js-sdk"
  },
  "files": [
    "types",
    "componentize.js",
    "componentize-cli.js",
    "lib/*.wasm",
    "lib/*.js",
    "src",
    "index.d.ts",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "test": "NODE_ENV=test jest -c ./config/jest/jest.config.js",
    "test:solo": "NODE_ENV=test jest -c ./config/jest/jest.config.js --",
    "build": "npm-run-all -p build:runtime build:js -s build:wasm",
    "build:componentize": "node esbuild/componentize.js ",
    "build:runtime": "make -j8 -C fastedge-runtime/cbindings && cp fastedge-runtime/cbindings/*.wasm ./lib",
    "build:wasm": "node build-examples-config.js",
    "build:js": "node esbuild/js-builtins.js",
    "build:debug": "DEBUG=true make -j8 -C execution_env/fastedge-runtime && cp execution_env/fastedge-runtime/*.wasm .",
    "generate:wit-world": "make -j8 -C fastedge-runtime/cbindings generate-wit-bindgen"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.9",
    "@gmrchk/cli-testing-library": "^0.1.2",
    "@jest/transform": "^29.7.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-transform-import-meta": "^2.2.1",
    "esbuild-plugin-polyfill-node": "^0.3.0",
    "eslint": "^8.56.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-ban": "^1.6.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.6.3",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-testing-library": "^6.2.0",
    "eslint-plugin-unicorn": "^50.0.1",
    "jest": "^29.7.0",
    "tsd": "^0.30.4",
    "typescript": "^5.0"
  },
  "dependencies": {
    "@bytecodealliance/jco": "^0.10.0",
    "@bytecodealliance/wizer": "^3.0.1",
    "acorn": "^8.8.2",
    "acorn-walk": "^8.2.0",
    "esbuild": "^0.17.18",
    "event-target-polyfill": "^0.0.4",
    "magic-string": "^0.30.0",
    "npm-run-all": "^4.1.5",
    "regexpu-core": "^5.3.2"
  }
}
